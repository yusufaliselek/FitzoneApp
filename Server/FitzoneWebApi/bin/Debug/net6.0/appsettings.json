{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "./logs/log-.txt",
          "rollingInterval": "Day"
        }
      },
      {
        "Name": "Seq",
        "Application": "Fitness API",
        "Args": { "serverUrl": "http://localhost:5341" }
      }
    ]
  },
  "ConnectionStrings": {
    "DBContext": "Server=(local)\\MSSQLSERVERENES ;Database=FitnessDB ;User Id=sa;Password=1234; Connect Timeout=5;Encrypt=False;TrustServerCertificate=True;MultiSubnetFailover=False;"
  },
  "AllowedHosts": "*"
}
