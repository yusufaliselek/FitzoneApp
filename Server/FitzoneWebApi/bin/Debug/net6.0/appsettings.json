{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime" :  "Information"
      },
      "WriteTo": [
        {
          "Name": "File",
          "Args": {
            "path": "./logs/log-txt",
            "rollingInterval": "Day"
          }
        },
        {
          "Name": "File",
          "Args": { "serverUrl": "http://localhost:5341" }
          }

      ]
    }
  },
  "ConnectionStrings": {
    "DBContext": "Server=(local)\\**** ;Database=**** ;User Id=****;Password=****; Connect Timeout=30;Encrypt=False;TrustServerCertificate=True;MultiSubnetFailover=False;"
  },
  "AllowedHosts": "*"
}
